<Window x:Class="Chess_Project.GameOver"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chess_Project"
        mc:Ignorable="d"
        Title="Game Over" Height="200" Width="500">
    <Grid>
        <TextBlock Text="Game Over" Margin="0,0,0,60" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Sans Serif Collection" FontWeight="Bold" Width="140" TextAlignment="Center" FontSize="24"/>
        <TextBlock Name="WinnerText" Margin="0,60,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Sans Serif Collection" FontWeight="Regular" Width="440" TextAlignment="Center" FontSize="20" Height="70" LineHeight="20" TextWrapping="Wrap" LineStackingStrategy="BlockLineHeight"/>
    </Grid>
</Window>
